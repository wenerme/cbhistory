<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <filter>
        <filter-name>server-madvoc-filter</filter-name>
        <filter-class>me.wener.cbhistory.server.madvoc.ace.AceServerFilter</filter-class>

        <init-param>
            <param-name>madvoc.webapp</param-name>
            <param-value>me.wener.cbhistory.server.madvoc.MadvocApp</param-value>
        </init-param>
        <init-param>
            <param-name>madvoc.configurator</param-name>
            <param-value>me.wener.cbhistory.server.madvoc.ace.ServerMadvocConfigurator</param-value>
        </init-param>

    </filter>
    <filter-mapping>
        <filter-name>server-madvoc-filter</filter-name>
        <url-pattern>/*</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>INCLUDE</dispatcher>
    </filter-mapping>


</web-app>